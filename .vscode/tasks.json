{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${workspaceFolder}\\broken-key.exe",
                "${workspaceFolder}\\broken-key.cpp",
                "${workspaceFolder}\\AST\\ASTNode.cpp",
                "${workspaceFolder}\\AST\\Function.cpp",
                "${workspaceFolder}\\AST\\Body.cpp",
                "${workspaceFolder}\\AST\\Statements.cpp"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}